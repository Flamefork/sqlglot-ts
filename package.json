{
	"name": "sqlglot-ts",
	"version": "0.1.1",
	"description": "SQL parser and transpiler for TypeScript - port of SQLGlot",
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		},
		"./dialects": {
			"types": "./dist/dialects/index.d.mts",
			"import": "./dist/dialects/index.mjs",
			"default": "./dist/dialects/index.mjs"
		},
		"./dialects/*": {
			"types": "./dist/dialects/*.d.mts",
			"import": "./dist/dialects/*.mjs",
			"default": "./dist/dialects/*.mjs"
		},
		"./expressions": {
			"types": "./dist/expressions.generated.d.mts",
			"import": "./dist/expressions.generated.mjs",
			"default": "./dist/expressions.generated.mjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"pytest": "uv run --directory tools pytest ../sqlglot/tests/dialects/ -v --tb=short -c pyproject.toml -p conftest --import-mode=importlib -k 'not test_lazy_load'",
		"test": "oxfmt --check src && oxlint src && tsc --noEmit && npm run pytest -- -k TestDuckDB",
		"fix": "oxfmt src && oxlint --fix src",
		"generate": "uv run --directory tools generate.py",
		"release": "sh tools/release.sh"
	},
	"keywords": [
		"sql",
		"parser",
		"transpiler",
		"ast",
		"duckdb"
	],
	"author": "Ilia Ablamonov",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Flamefork/sqlglot-ts.git"
	},
	"homepage": "https://github.com/Flamefork/sqlglot-ts#readme",
	"bugs": {
		"url": "https://github.com/Flamefork/sqlglot-ts/issues"
	},
	"sideEffects": [
		"./dist/index.mjs",
		"./dist/dialects/*.mjs"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.0.0",
		"oxfmt": "0.34.0",
		"oxlint": "1.49.0",
		"tsdown": "0.20.3",
		"typescript": "^5.3.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
